package mjsd2;

import java.util.ArrayList;

public class Alumnos {
	ArrayList <Alumno> Alumnos;
	public Alumnos() {
		Alumnos = new ArrayList<>();
	};
	
	public void addAlumno (String name, String lastname, ArrayList<Integer> Results) {
		Alumno newAlumno = new Alumno(name, lastname);
		for (Integer Result : Results) {
			newAlumno.addResult(Result);
		};
		Alumnos.add(newAlumno);
	};
	
	public void getPromoted
}
